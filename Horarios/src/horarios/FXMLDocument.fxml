      <BorderPane prefHeight="371.0" prefWidth="656.0">
         <center>
            <TableView fx:id="tabla" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <items>
                <Button mnemonicParsing="false" text="Ver configuración" />
                  <Button mnemonicParsing="false" onAction="#seleccionarArchivo" text="Cargar configuración" />
                  <Button fx:id="generar" mnemonicParsing="false" onAction="#generarHorario" text="Generar" />
